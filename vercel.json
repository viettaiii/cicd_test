{
  "version": 2,
  "builds": [
    {
      "src": "cicd_test/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "pythonVersion": "3.11",
        "installCommand": "pip install -r requirements.txt",
        "buildCommand": null
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "cicd_test/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "cicd_test.settings",
    "PYTHONPATH": "."
  }
}